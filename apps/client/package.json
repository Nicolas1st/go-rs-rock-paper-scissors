{
  "name": "client",
  "version": "1.0.0",
  "author": "Dmitry Zakharov <dzakh.dev@gmail.com>",
  "type": "module",
  "scripts": {
    "build": "npm run res:build",
    "lint": "npm run lint:rescript-format && npm run lint:stdlib && npm run lint:eslint",
    "lint:eslint": "eslint .",
    "lint:rescript-format": "rescript format -check",
    "lint:stdlib": "rescript-stdlib-vendorer lint",
    "res:build": "rescript",
    "res": "rescript -w",
    "start": "node ./src/Main.bs.mjs",
    "test": "ava"
  },
  "ava": {
    "files": [
      "src/**/*_test.bs.mjs"
    ]
  },
  "dependencies": {
    "@rescript/core": "1.5.0",
    "inquirer": "8.2.4",
    "rescript": "11.1.2",
    "rescript-envsafe": "4.1.0",
    "rescript-rest": "0.4.1",
    "rescript-schema": "7.0.1",
    "undici": "5.28.4"
  },
  "devDependencies": {
    "@dzakh/rescript-ava": "3.0.0",
    "ava": "5.2.0",
    "rescript-stdlib-vendorer": "1.1.0"
  }
}
